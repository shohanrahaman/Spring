<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">




	<bean id="patient" class="com.shohan.CNamespace.Patient" scope="prototype"
		c:id="101" c:name="shohan" c:nationalId="785624" c:address-ref="address">
	</bean>
	<bean id="address" class="com.shohan.CNamespace.Address" p:street="Mirpur Road"
		p:postcode="1206">

	</bean>
</beans>

<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">




	<bean id="patient" class="com.shohan.PNamespace.Patient"
		scope="prototype">
		<property name="id" value="101"></property>
		<property name="name" value="shohan"></property>
		<property name="address" ref="address"></property>
		<property name="nationalId" value="586977"></property>
	</bean>
	<bean id="address" class="com.shohan.PNamespace.Address" p:street="Mirpur Road" p:postcode="1206"> 
	
	</bean>
</beans>
